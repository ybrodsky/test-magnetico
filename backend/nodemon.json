{
  "restartable": "rs",
  "ignore": ["test/*", "*.txt", "src/services/*.json", "*.xml"],
  "exec": "ts-node ./src/index.ts",
  "watch": ["src", "config"],
  "env": {
    "NODE_ENV": "development",
    "DEBUG": "admin:*",
    "DEBUG_COLORS": "true",
    "DEBUG_DEPTH": 5
  },
  "ext": "ts js json"
}
